{
  "name": "@mudssky/jsutils",
  "version": "1.26.0",
  "description": "个人通用js库",
  "keywords": [
    "util"
  ],
  "homepage": "https://github.com/mudssky/jsutils",
  "bugs": {
    "url": "https://github.com/mudssky/jsutils/issues",
    "email": "mudssky@gmail.com"
  },
  "license": "MIT",
  "author": {
    "name": "mudssky",
    "email": "mudssky@gmail.com",
    "url": "https://github.com/mudssky/jsutils"
  },
  "contributors": [
    {
      "name": "mudssky",
      "email": "mudssky@gmail.com"
    }
  ],
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "require": {
        "types": "./dist/typescts/index.d.cts",
        "default": "./dist/cjs/index.cjs"
      },
      "types": "./dist/types/index.d.ts",
      "import": "./dist/esm/index.js",
      "default": "./dist/esm/index.js"
    }
  },
  "main": "./dist/cjs/index.cjs",
  "module": "./dist/esm/index.js",
  "typings": "./dist/types/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "prebuild": "pnpm clean:dist&&pnpm copy:style",
    "build": "rollup -c",
    "check": "biome check .",
    "check:apply": "biome check --apply .",
    "ci:check": "pnpm typecheck && pnpm test && pnpm lintfix ",
    "clean:dist": "pwsh -File scripts.ps1 -JobType CleanDist",
    "commit": "git add .&&git cz",
    "copy:style": "pwsh -File scripts.ps1 -JobType CopyStyle",
    "coverage": "vitest run --coverage",
    "dev": "rollup -c rollup.config.dev.js",
    "dev:watch": "rollup -c rollup.config.dev.js --watch",
    "docs:build": "vitepress build vitedocs",
    "docs:debug": "api-extractor run --local --verbose --diagnostics && api-documenter markdown -i temp -o docs",
    "docs:dev": "vitepress dev vitedocs",
    "docs:extract": "api-extractor run --local --verbose && api-documenter markdown -i temp -o vitedocs/docs",
    "docs:preview": "vitepress preview vitedocs",
    "format": "biome format --write .",
    "lint": "eslint --ext .js,.ts ./",
    "lintfix": "eslint --ext .js,.ts ./ --fix",
    "prepare": "husky",
    "prepublishOnly": "pnpm build",
    "push": "pnpm commit&&git push",
    "test": "vitest --run --typecheck",
    "test:ui": "vitest --ui --typecheck",
    "test:watch": "vitest",
    "trace": "tsc dist/types/index.d.ts --traceResolution > trace.log",
    "typecheck": "tsc --noEmit",
    "typedoc:gen": "typedoc src/index.ts --out typedoc",
    "typedoc:json": "typedoc src/index.ts --json typedoc/typedoc.json",
    "typedoc:watch": "pwsh -File scripts.ps1 -JobType WatchTypedoc"
  },
  "lint-staged": {
    "*.{md,json}": [
      "prettier --write --no-error-on-unmatched-pattern"
    ],
    "*.{css,less}": [
      "stylelint --fix",
      "prettier --write"
    ],
    "*.{js,jsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --parser=typescript --write"
    ]
  },
  "config": {
    "commitizen": {
      "path": "git-cz"
    }
  },
  "dependencies": {
    "clsx": "^2.1.1",
    "tailwind-merge": "^3.3.1"
  },
  "devDependencies": {
    "@babel/core": "^7.28.0",
    "@babel/preset-env": "^7.28.0",
    "@biomejs/biome": "^2.1.2",
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@eslint/js": "^9.32.0",
    "@microsoft/api-documenter": "^7.26.30",
    "@microsoft/api-extractor": "^7.52.9",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-commonjs": "^28.0.6",
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^12.1.4",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@types/node": "^24.1.0",
    "@typescript-eslint/eslint-plugin": "^8.38.0",
    "@typescript-eslint/parser": "^8.38.0",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "cz-conventional-changelog": "^3.3.0",
    "esbuild": "^0.25.8",
    "eslint": "^9.32.0",
    "eslint-plugin-tsdoc": "^0.4.0",
    "fast-glob": "^3.3.3",
    "git-cz": "^4.9.0",
    "happy-dom": "^18.0.1",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.6.2",
    "prettier-plugin-organize-imports": "^4.2.0",
    "prettier-plugin-packagejson": "^2.5.19",
    "rollup": "^4.45.1",
    "rollup-plugin-dts": "^6.2.1",
    "rollup-plugin-esbuild": "^6.2.1",
    "rollup-plugin-node-builtins": "^2.1.2",
    "rollup-plugin-node-globals": "^1.4.0",
    "rollup-plugin-typescript2": "^0.36.0",
    "semantic-release": "^24.2.7",
    "stylelint": "^16.22.0",
    "tslib": "^2.8.1",
    "typedoc": "^0.28.7",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.38.0",
    "vitepress": "1.6.3",
    "vitest": "^3.2.4"
  },
  "packageManager": "pnpm@10.13.1+sha512.37ebf1a5c7a30d5fabe0c5df44ee8da4c965ca0c5af3dbab28c3a1681b70a256218d05c81c9c0dcf767ef6b8551eb5b960042b9ed4300c59242336377e01cfad",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}
